{% block templates %}
<template id="hs-search-field-template">
    <form action="/wosn/" method="get" class="form-horizontal searchForm" name="hs-search-form">
        <div class="search-field input-group search-top">
            <input type="text" class="form-control search-field" name="query"
                placeholder="Search by keyword, part #, etc." hawksearch-input />
            <button class="btn btn-search fastclick" type="submit"> <i class="fa fa-search red"></i> </button>
        </div>
        <hawksearch-autocomplete></hawksearch-autocomplete>
    </form>
</template>

<template id="hs-autocomplete-template">
    <div class="autocomplete"> {%if queries.results.length%}
        <div class="autocomplete__title autocomplete__title--queries">{{queries.title}}</div>
        <ul class="row autocomplete__list">
            {% for query in  queries.results %}
            <li> <a hawksearch-query="{{query}}" href="{{url}}">{{query}}</a> </li>
            {% endfor %}
        </ul>
        {% endif %}
        {% if categories.results.length %}
        <div class="autocomplete__title autocomplete__title--categories">{{categories.title}}</div>
        <ul class="autocomplete__list">
            {% for result in  categories.results %}
            <li> <a hawksearch-category-field="{{field}}" hawksearch-category-value="{{value}}"
                    {# href="{% url %}">{% html title %}</a> </li> #} {% endfor %} </ul>
                    {% endif %}

                    {% if products.results.length %}
                    <div class="autocomplete__title autocomplete__title--products">{{products.title}}</div>
                    <ul class="autocomplete__list autocomplete__products">
                        {% for result in products.results %}
                        <li class="autocomplete__product"> <a hawksearch-product="{{id}}"
                                href="{{autocompleteItem_productUrl}}">
                                <div class="autocomplete__product__image"> <img hawksearch-image
                                        src="{{autocompleteItem_imageUrl}}" alt="" /> </div>
                                <div class="autocomplete__product__content">
                                    {# <span #}
                                    {# class="autocomplete__product__title">{{ html(autocompleteItem_title) }}</span> </div> #}
                            </a> </li>
                        {% endfor %}
                    </ul>
                    {% endif %}

                    {% if content.results.length %}
                    <div class="autocomplete__title autocomplete__title--content">{{content.title}}</div>
                    <ul class="autocomplete__list">
                        {% for result in content.results %}
                        {# <li> <a hawksearch-content="{{result.id}}" href="{{result.url}}">{{result.html result.title}}</a> </li> #}
                        {% endfor %}
                    </ul>
                    {% endif %}
                    <div class="autocomplete__title autocomplete__view-all"> <a hawksearch-view-all
                            onclick="event.preventDefault(); this.closest('form').submit();">{{viewAllText}}</a>
                        <div style="font-weight: normal; margin-top: 3px; font-size: 0.85em;">{{totalRecords}}
                            products(s)</div>
                    </div>
    </div>
</template>


{% endblock %}

</html>